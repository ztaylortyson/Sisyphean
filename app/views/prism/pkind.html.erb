<h1>Prism#pkind</h1>
<hr>
<table class="mytables">
	<tr>
		<thead>
		<th>Source</th>
		<th>Year</th>
		<th>Contract Type</th>
		<th>Member ID</th>
		<th>Member</th>
		<th>Job Title</th>
		<th>Discharge</th>
		<th>Notes</th>
	</thead>
	</tr>
	<% @contracts.each do |c| %>
	<% gmember(c) %>
	<tr>
		<td><%= link_to "#{c.file.filename}", url_for(c.file), class: "butt round", target: "_blank" if c.file.content_type == 'application/pdf' %></td>
    
		<td><%= c.year %></td>
		<td><%= c.kind %></td>
		<td><%= c.member_id %></td>
		<td><% if @member %><%= @member.lname %>, <%= @member.fname %><% end %></td>
		<td><% if @member %><%= link_to "#{@member.job_title}", prism_pjob_title_path(member: @member) %><% end %></td>
		<td><% if @member %><%= @member.dod %><% end %></td>
		<td><%= c.notes %></td>
	</tr>
	<% end %> 

</table>
<%= link_to "Back", contracts_path, class: "button" %>
